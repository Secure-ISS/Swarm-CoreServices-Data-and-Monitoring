{
  "dashboard": {
    "title": "Distributed PostgreSQL - Performance Overview",
    "tags": ["postgresql", "ruvector", "performance"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Vector Search Latency (p95)",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "p95 latency",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(vector_search_duration_seconds_bucket[5m]))",
            "legendFormat": "p99 latency",
            "refId": "B"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Latency"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [0.05]},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "High Vector Search Latency",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 2,
        "title": "Queries Per Second",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "rate(vector_search_results_total[1m])",
            "legendFormat": "{{namespace}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "QPS"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Database Connections",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "db_connections_active",
            "legendFormat": "Active",
            "refId": "A"
          },
          {
            "expr": "db_connections_idle",
            "legendFormat": "Idle",
            "refId": "B"
          },
          {
            "expr": "db_connections_total",
            "legendFormat": "Total",
            "refId": "C"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Connections"},
          {"format": "short"}
        ]
      },
      {
        "id": 4,
        "title": "Cache Hit Ratio",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "cache_hit_ratio",
            "legendFormat": "Hit Ratio",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "percentunit", "label": "Hit Ratio", "min": 0, "max": 1},
          {"format": "short"}
        ],
        "thresholds": [
          {"value": 0.9, "colorMode": "critical", "op": "lt"}
        ]
      },
      {
        "id": 5,
        "title": "Replication Lag",
        "type": "graph",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "replication_lag_seconds",
            "legendFormat": "{{replica_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "s", "label": "Lag"},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"type": "gt", "params": [10]},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"type": "avg"},
              "type": "query"
            }
          ],
          "name": "High Replication Lag"
        }
      },
      {
        "id": 6,
        "title": "Table Sizes",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "table_size_bytes",
            "legendFormat": "{{table_name}} ({{schema}})",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "bytes"
          }
        }
      },
      {
        "id": 7,
        "title": "Table Row Counts",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "table_row_count",
            "legendFormat": "{{table_name}}",
            "refId": "A"
          }
        ],
        "options": {
          "displayMode": "gradient",
          "orientation": "horizontal",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "rate(vector_search_errors_total[5m])",
            "legendFormat": "{{namespace}} - {{error_type}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Errors/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 9,
        "title": "Index Sizes (HNSW)",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "index_size_bytes",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "index_name": "Index",
                "table_name": "Table",
                "Value": "Size"
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [
            {"displayName": "Size", "desc": true}
          ]
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "namespace",
          "type": "query",
          "datasource": "Prometheus",
          "query": "label_values(vector_search_results_total, namespace)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}
